{
    "$schema": "http://json.schemastore.org/template",
    "author": "Tairan Wang <tairan.wang@gmail.com>",
    "classifications": [
        "Web/MVC"
    ],
    "description": "The startup template which is properly configured to jump start to your project.",
    "identity": "Volo.Abp.Service",
    "groupIdentity": "Volo.Abp",
    "preferNameDirectory": true,
    "name": "ABP ASP.NET Core Service Template",
    "shortName": "abpservice",
    "tags": {
        "language": "C#"
    },
    "sourceName": "MyCompanyName.MyProjectName",
    "sources": [
        {
            "source": ".",
            "target": ".",
            "exclude": [
                "**/[Bb]in/**",
                "**/[Oo]bj/**",
                ".template.config/**/*",
                ".vscode/**/*",
                "**/*.filelist",
                "**/*.user",
                "**/*.lock.json"
            ]
        }
    ]
}